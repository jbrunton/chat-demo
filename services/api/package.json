{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "db:init": "NODE_ENV=development ts-node ./scripts/db/init.ts",
    "db:drop": "NODE_ENV=development ts-node ./scripts/db/drop.ts",
    "db:test:drop": "NODE_ENV=test ts-node ./scripts/db/drop.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --max-warnings 0",
    "test": "jest",
    "test:int": "jest --config ./jest.int.config.js",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:mutate": "stryker run",
    "test:e2e": "jest --config ./test/jest-e2e.config.cjs",
    "docs:generate": "typedoc",
    "docs:serve": "http-server docs"
  },
  "dependencies": {
    "@anatine/zod-nestjs": "2.0.9",
    "@anatine/zod-openapi": "2.2.6",
    "@aws-sdk/client-dynamodb": "3.238.0",
    "@aws-sdk/credential-provider-node": "3.238.0",
    "@aws-sdk/lib-dynamodb": "3.238.0",
    "@casl/ability": "6.3.3",
    "@faker-js/faker": "7.6.0",
    "@nestjs/common": "9.4.2",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "7.4.0",
    "async-cache-dedupe": "1.8.0",
    "auth0": "3.0.1",
    "cache-manager": "5.1.4",
    "dynamodb-onetable": "2.5.1",
    "express-jwt": "7.7.7",
    "jwks-rsa": "3.0.0",
    "node-fetch": "2.6.7",
    "openapi3-ts": "4.3.3",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "rambda": "7.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@stryker-mutator/core": "7.0.2",
    "@stryker-mutator/jest-runner": "7.0.2",
    "@types/auth0": "2.35.8",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/node-fetch": "2.6.2",
    "@types/passport-jwt": "3.0.8",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "8.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-boundaries": "3.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "^4.0.0",
    "http-server": "14.1.1",
    "jest": "29.7.0",
    "jest-fail-on-console": "3.1.1",
    "jest-mock-extended": "3.0.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.2.4",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typedoc": "0.26.5",
    "typescript": "^4.7.4"
  }
}
